<footer class="footer">
  {{#infoBar}}
  <div class="container">
    <div class="info-bar">
      <div class="info-bar-col">
        <h5 class="info-bar-title">FREE SHIPPING &amp; RETURN</h5>
        <p>Free shipping on all orders over $99</p>
      </div>
      <div class="info-bar-col">
        <h5 class="info-bar-title">MONEY BACK GUARANTEE</h5>
        <p>100% money back guarantee</p>
      </div>
      <div class="info-bar-col">
        <h5 class="info-bar-title">ONLINE SUPPORT 24/7</h5>
        <p>Highly customer satisfaction</p>
      </div>
    </div>
  </div>
  {{/infoBar}}
  <div class="footer-inner">
    <div class="container">
      <div class="row">
        <div class="col-sm-6 col-md-3">
          <div class="widget widget-about">
            <h4 class="widget-title">Contact</h4>

            <address>
              <span>{{account.address_1}} {{account.address_2}}</span>
              <span>{{account.city}}, {{account.state}} {{account.zip_code}}</span>
              {{#account.phone_number}}
              <span>{{account.phone_number}}</span>
              {{/account.phone_number}}
              {{#fetchProducts.email_address}}
              <a href="{{fetchProducts.emailAddress}}">{{fetchProducts.email_addre}}</a>
              {{/fetchProducts.email_address}}
            </address>
          </div>
        </div>

        <div class="col-sm-6 col-md-3">
          {{#newsletter.footer}}
          <div class="widget">
            <h4 class="widget-title">Links</h4>

            <ul class="links">
              <li><a href="#">Account</a></li>
          </div>
          <div class="widget widget-newsletter">
            <h4 class="widget-title">Newsletter</h4>
            <p>Signup for our newsletter</p>

            <form action="#">
              <div class="form-group">
                <input type="email" class="form-control" placeholder="Your Email" required>
                <input type="submit" value="GO" class="btn">
              </div>
            </form>
          </div>
          {{/newsletter.footer}}
        </div>

        <div class="col-sm-6 col-md-3">
          <div class="widget">
            <h4 class="widget-title">Collection</h4>

            <ul class="links">
              {{#categories}}
                <li>
                  <a href="{{categoryLink}}">{{categoryName}}</a>
                </li>
              {{/categories}}
            </ul>
          </div>
        </div>

        <div class="clearfix visible-sm"></div>

        <div class="col-sm-6 col-md-3">
          <div class="widget">
            <h4 class="widget-title">Links</h4>

            <ul class="links">
              <li><a href="/account">Account</a></li>
              <li><a href="/contact">Contact Us</a></li>
          </div>
        </div>
      </div>
    </div>
  </div>

  <div class="footer-bottom">
    <div class="container">
      <p class="copyright">&copy; {{account.company}}, {{pageInfo.currentYear}} All Rights Reserved</p>
    </div>
  </div>
</footer>
</div>
<script src="/vendor/assets/js/plugins.js"></script>
<script src="/vendor/assets/js/main.js"></script>
<script src="https://checkout.stripe.com/checkout.js"></script>
{{^isDemo}}
<script src="/js/ecommerce.bundle.min.js"></script>
{{/isDemo}}
{{#isDemo}}
<script src="http://localhost:8080/ecommerce/static/js/ecommerce.bundle.js"></script>
{{/isDemo}}
</body>
</html>
